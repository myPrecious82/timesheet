        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <!-- email settings -->
    <add key="EmailSmtpHost" value="SMTPR.illinois.gov"/>
    <add key="EmailSubject" value="Timesheet is ready to print" />
    <add key="EmailBody" value="" />
    <!-- template filenames -->
    <add key="TemplateFileName" value="Templates\Template - Semi-Monthly Time Sheet.xlsx" />
